# Uncomment the next line to define a global platform for your project
platform :ios, '11.1'
workspace 'acara.in'
target 'acara.in' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for acara.in

  pod 'JSONWebToken'
  pod 'GoogleMaps'
  pod 'GooglePlaces'
  pod 'SwiftyJSON'
  pod 'Alamofire'
  target 'acara.inTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'acara.inUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
